{
	"name": "standalone-genug",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "railway run vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
