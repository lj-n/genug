{
	"name": "genug",
	"version": "1.0.3",
	"private": true,
	"scripts": {
		"start": "node build",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"migrations:generate": "drizzle-kit generate:sqlite",
		"migrations:push": "vite-node migrations/migrate.ts",
		"create:user": "TEST=true vite-node create.user.ts",
		"test": "vitest watch",
		"test:ci": "vitest --run"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/typography": "^0.5.10",
		"@types/better-sqlite3": "^7.6.6",
		"@types/feather-icons": "^4.29.3",
		"@types/nodemailer": "^6.4.11",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@unocss/extractor-svelte": "^0.57.7",
		"@unocss/reset": "^0.57.7",
		"@unocss/transformer-variant-group": "^0.57.7",
		"browserslist": "^4.22.1",
		"drizzle-zod": "^0.5.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"feather-icons": "^4.29.1",
		"lightningcss": "^1.22.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"unocss": "^0.57.7",
		"vite": "^5.0.0",
		"vitest": "^1.1.0",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-sqlite": "^3.0.0",
		"@sveltejs/adapter-node": "^2.0.0",
		"better-sqlite3": "^8.7.0",
		"chart.js": "^4.4.0",
		"drizzle-kit": "^0.19.13",
		"drizzle-orm": "^0.28.6",
		"lucia": "^3.0.1",
		"oslo": "^1.1.3",
		"vite-node": "^0.34.6"
	}
}
