		"@types/feather-icons": "^4.29.4",
		"@types/nodemailer": "^6.4.14",
		"@typescript-eslint/eslint-plugin": "^7.7.0",
		"@typescript-eslint/parser": "^7.1.0",
		"@unocss/extractor-svelte": "^0.59.2",
		"@unocss/reset": "^0.58.6",
